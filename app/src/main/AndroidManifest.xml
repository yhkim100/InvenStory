<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="ece651.duke.invenstory">
    <uses-feature android:name ="android.hardware.camera2" android:required="false"> </uses-feature>

    <uses-permission android:name="android.permission.CAMERA" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE"/>

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/Theme.AppCompat.Light.NoActionBar">
        <activity android:name=".Splash">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".HomePage"></activity>
        <activity android:name=".ShowList"/>
        <activity android:name=".DetailedListView"/>
        <activity android:name=".PhotoActivity"/>
        <activity android:name=".Register"/>
        <activity android:name=".Login"/>
    </application>

</manifest>
